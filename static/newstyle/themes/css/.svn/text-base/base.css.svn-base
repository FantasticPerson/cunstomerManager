/*返回首页*/
.top_tool a.home:link,.top_tool a.home:visited {
	background: url(../default/images/headerBtn_home.gif) 0 0 no-repeat;
}

.top_tool a.home:hover {
	background: url(../default/images/headerBtn_home.gif) 0 -30px no-repeat;
}

.top_tool a.home:active {
	background: url(../default/images/headerBtn_home.gif) 0 -60px no-repeat;
}
/*修改密码*/
.top_tool a.modifyPwd:link,.top_tool a.modifyPwd:visited {
	background: url(../default/images/headerBtn_modifyPwd.gif) 0 0 no-repeat;
}

.top_tool a.modifyPwd:hover {
	background: url(../default/images/headerBtn_modifyPwd.gif) 0 -30px
		no-repeat;
}

.top_tool a.modifyPwd:active {
	background: url(../default/images/headerBtn_modifyPwd.gif) 0 -60px
		no-repeat;
}
/*退出系统*/
.top_tool a.exit:link,.top_tool a.exit:visited {
	background: url(../default/images/headerBtn_exit.gif) 0 0 no-repeat;
}

.top_tool a.exit:hover {
	background: url(../default/images/headerBtn_exit.gif) 0 -30px no-repeat;
	color: #610000 !important;
}

.top_tool a.exit:active {
	background: url(../default/images/headerBtn_exit.gif) 0 -60px no-repeat;
	color: #610000 !important;
}


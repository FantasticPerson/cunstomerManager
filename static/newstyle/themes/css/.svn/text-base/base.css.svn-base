/* 框架---------------------------------------------------------------------*/
.content_box {
	width: 98%;
	height: 100%;
	margin: 0 auto
}

.mainleft {
	HEIGHT: 100%;
	VISIBILITY: inherit;
	width: 200px;
	Z-INDEX: 2;
	overflow: hidden
}

.maincenter {
	width: 8px;
	height: 100%;
	background-color: #6699CC;
	COLOR: white;
	CURSOR: hand;
	FONT-FAMILY: Webdings;
	FONT-SIZE: 6pt
}

.mainright {
	HEIGHT: 100%;
	VISIBILITY: inherit;
	WIDTH: 100%;
	Z-INDEX: 1
}
/*===============左侧菜单样式===============*/
.wrap-menu {
	margin: 5px auto;
	overflow: auto;
	width: 190px;
	background: #f8fafa;
	font: 12px/1.5 Tahoma, Arial, sans-serif
}

.wrap-menu ul {
	list-style: none;
	margin: 0;
	padding: 0;
}

.wrap-menu ul li {
	text-indent: 22px;
	white-space: nowrap;
}

.wrap-menu ul li h2 {
	background: url(../default/images/mainMenu_close.gif) no-repeat;
	cursor: pointer;
	height: 100%;
	margin: 1px 0 0 0;
	font: 12px/31px '宋体';
	font-weight: bold;
	color: #fff;
	height: 32px;
	line-height: 32px;
}

.wrap-menu ul li a {
	display: block;
	outline: none;
	height: 25px;
	line-height: 25px;
	margin: 1px 0;
	color: #1A385C;
	color: #787878;
	text-decoration: none;
}

.wrap-menu ul li img {
	margin-right: 10px;
	margin-left: -17px;
	margin-top: 9px;
	width: 7px;
	height: 7px;
	background: url(../default/images/arrow.gif) no-repeat;
	border: none;
}

.wrap-menu ul li img.unfold {
	background-position: 0 -9px;
}

.wrap-menu ul li a:hover {
	background-color: #f2d79e;
	background-image: none;
}

.wrap-menu ul li ul {
	border: 1px solid #c9ced1;
	border-bottom: 0;
}

.wrap-menu ul li ul li {
	background: url(../default/images/subMenu.gif) repeat-x;
	border-bottom: 1px solid #c9ced1;
}

.wrap-menu ul li ul li ul {
	border: 0;
	background: #e9f2f9;
	border-top: 1px solid #c9ced1;
}

.wrap-menu ul li ul li ul li {
	background: #e9f2f9 !important;
	border: 0;
}

#tb_list .grid .gridHeader th,#tb_list .grid .gridTbody td{
    border-right: 1px dashed #cecece;
}
#tb_list .grid .gridHeader th{
    height:30px;
    line-height: 30px;
    text-align: center;
    color:#020202;
}
#tb_list .grid .gridHeader th.hover{
    background: url(../../default/images/grid/CM_thbg.jpg) 0 0 repeat-x;
}
#tb_list .grid .gridHeader th div{
    height:30px;
    line-height: 30px;
}
#tb_list .grid .gridHeader th span{
    line-height: 30px;
}
#tb_list .grid .gridHeader th input{
    position: relative;
    top:8px;
}
#tb_list .grid .gridHeader th.last_col{
    border-right: 1px solid #cecece;
}
#tb_list .grid .gridTbody td{
    border-bottom: 1px dashed #cecece;
    height:30px;
    line-height: 30px;
}
#tb_list .grid .gridTbody td.last_col{
    border-right: 1px solid #cecece;
}
#tb_list .grid .gridTbody tr.last_row td{
    border-bottom:1px solid #cecece;
}
/****operation icon*******/
.list_o{
    border-bottom: 1px solid #cecece;
}
.list_o a{
    display: inline-block;
    height: 30px;
    line-height: 30px;
    color: #787878;
    position: relative;
    padding-left: 15px;
    margin-left: 15px;
}
.list_o i{
    display: inline-block;
    position: absolute;
    left: 0;
    top: 0;
    background: url(../../default/images/icon_opt.png) no-repeat;
}
.list_o .list_o_1,.list_o .list_o_2{
    padding-left: 20px;
}
.list_o_1 i{
    background-position: 0 -214px;
    top:10px;
    width:15px;
    height:11px;

}
.list_o_2 i{
    background-position: 0 -227px;
    top:8px;
    width:15px;
    height:14px;

}
.list_o_3 i{
    width: 11px;
    height: 11px;
    background-position: 0 -47px;
    top: 9px;
}
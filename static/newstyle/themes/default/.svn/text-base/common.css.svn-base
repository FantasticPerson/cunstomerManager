/******************左侧导航******************************/
.guidewrap{
    width:96px;
    height:100%;
    position:absolute;
    left:0;
    top:0;
    background-color: #f2f2f2;
}
.guidecontent{
    width:100%;
}
.uparrow,.downarrow{
    display: block;
    height:22px;
    width:100%;
    background:#fff;
}
.slidewrap{
    width:100%;
    overflow: hidden;
}
#guideList{
    overflow: hidden;
    position:relative;
}
#guideList li{
    height:101px;
    overflow: hidden;
}
#guideList li a{
    width:83px;
